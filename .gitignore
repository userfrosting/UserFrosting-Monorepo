# Ignores Mac metadata.  You can configure this globally if you use a Mac: http://islegend.com/development/setting-global-gitignore-mac-windows/
.DS_Store

# Ignore developer resources
_meta/

# Ignore personal config files (dev)
app/.env

# Ignore lock files
composer.lock
package-lock.json

# Ignore log, cache, sessions and storage directories content
app/cache/*
!app/cache/.gitkeep
app/database/*
!app/database/.gitkeep
app/logs/*
!app/logs/.gitkeep
app/sessions/*
!app/sessions/.gitkeep
app/storage/*
!app/storage/.gitkeep

# Ignore Composer vendor
vendor/

# Ignore node modules
node_modules/

# Ignore compiled assets
public/assets/

# Ignore dev utils cache
.phpunit.cache
.php_cs.cache

# Ignore api doc
api/

# Ignore test artifact
app/tests/Integration/Session/data/*
!app/tests/Integration/Session/data/.gitkeep
app/tests/Integration/Filesystem/storage/*
!app/tests/Integration/Filesystem/storage/.gitkeep
app/tests/Integration/Bakery/data/env/.env
